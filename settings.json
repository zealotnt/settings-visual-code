{
  "workbench.editor.wrapTabs": true,
  "workbench.panel.defaultLocation": "right",
  "workbench.tree.indent": 24,
  "sublimeTextKeymap.promptV3Features": true,
  "editor.fontSize": 12,
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  "editor.wordWrap": "on",
