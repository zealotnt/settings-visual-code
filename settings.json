  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Monokai",
  "window.title": "${activeEditorLong}${separator}${rootName}",
